{
    "compilerOptions": {
        "moduleResolution": "node",
        "alwaysStrict": true,
        "sourceMap": true,
        "target": "es5",
        "removeComments": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "stripInternal": true,
        "noFallthroughCasesInSwitch": true,
        "noEmitOnError": true,
        "jsx": "react",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "lib": ["dom", "es2015"]
    }
}
